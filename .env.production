# API URLs
VITE_API_URL=https://nerd-api.nerdslab.in
VITE_LAB_API_URL=https://lab.nerdslab.in
VITE_FRONTEND_URL=https://learn.nerdslab.in

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=false
VITE_ENABLE_LAB_FEATURES=true
VITE_ENABLE_COMMUNITY_FEATURES=true

# Security
VITE_CSP_ENABLED=true
VITE_HSTS_ENABLED=true

# Performance
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_CACHING=true
VITE_ENABLE_CACHE=true
VITE_CACHE_TTL=3600

# Build Settings
VITE_BUILD_SOURCEMAP=false
VITE_BUILD_MINIFY=true
VITE_BUILD_TARGET=es2015